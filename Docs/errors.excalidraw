      "id": "ODTofrsW9y7K7JstGwcpI",
      "type": "text",
      "x": 2473.529422752296,
      "y": -479.5868553572591,
      "width": 383.35595703125017,
      "height": 175,
      "angle": 0,
      "strokeColor": "#1e1e1e",
      "backgroundColor": "transparent",
